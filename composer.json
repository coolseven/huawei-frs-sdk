{
    "name": "coolseven/huawei-frs-sdk",
    "type": "library",
    "description": "Huawei Frs (Face Recognition Service) SDK for PHP",
    "keywords": ["huawei", "frs", "sdk"],
    "homepage": "https://github.com/coolseven/huaweicloud-sdk-php-frs",
    "license": "MIT",
    "authors": [
        {
            "name": "coolseven",
            "email": "cooleven2013@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit"
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "ramsey/uuid": "^3.9",
        "moontoast/math": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "HuaweiFrsSdk\\": "src/HuaweiFrsSdk"
        },
        "files": ["src/HuaweiFrsSdk/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "HuaweiFrsSdk\\": "tests/HuaweiFrsSdk"
        }
    }
}
